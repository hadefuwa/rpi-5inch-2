{
  "port": 3000,
  "host": "0.0.0.0",
  "cors": true,
  "gzip": true,
  "ext": "html",
  "headers": {
    "/*.html": {
      "Cache-Control": "no-store, must-revalidate",
      "Pragma": "no-cache",
      "Expires": "0"
    },
    "/sw.js": {
      "Cache-Control": "no-store, must-revalidate",
      "Pragma": "no-cache",
      "Expires": "0"
    },
    "/index.html": {
      "Cache-Control": "no-store, must-revalidate",
      "Pragma": "no-cache",
      "Expires": "0"
    },
    "/manifest.json": {
      "Cache-Control": "no-cache, must-revalidate"
    },
    "/assets/*": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/styles/*": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/js/*": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
}
